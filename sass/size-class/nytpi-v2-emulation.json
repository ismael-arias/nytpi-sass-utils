{
	"options": {
		"layout-types": [
			"pspx"
		],
		"config-pspx-ratio": 2,
		"default-layout-type": "pspx",
		"outer-margin": 0,
		"pspx-ratio": 2
	},
	"classes": {
		"mobile-portrait": {
			"pspx": {
				"columns": 640,
				"gutters": 0
			},
			"media": "0px 539px"
		},
		"tablet-portrait": {
			"pspx": {
				"columns": 1536,
				"gutters": 0
			},
			"media": "540px 853px"
		},
		"tablet-landscape": {
			"pspx": {
				"columns": 2048,
				"gutters": 0
			},
			"media": "854px 1279px"
		},
		"desktop": {
			"pspx": {
				"columns": 2560,
				"gutters": 0
			},
			"media": "1280px"
		},
		"shell": {
			"pspx": {
				"columns": 3210,
				"gutters": 0
			},
			"media": "1605px"
		}
	},
	"families": {
		"compact": "mobile-portrait",
		"regular": "desktop for desktop to tablet-portrait",
		"all-mobile": "mobile-portrait to desktop",
		"all-desktop": "desktop to mobile-portrait"
	},
	"breakpoints": {
		"between-desktop-and-shell": "1281px 1604px"
	}
}